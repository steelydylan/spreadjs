{
  "name": "spread-js",
  "version": "0.0.1",
  "dependencies": {
    "a-template": "0.0.8",
    "clone": "^2.0.0",
    "jquery": "^3.1.1",
    "stringify": "^5.1.0",
    "zepto-browserify": "^1.1.6-1"
  },
  "scripts": {
    "build": "browserify ./src/index.js -t [ babelify --presets [ es2015 ] ] -t [ stringify --extensions [.html .css] ] --standalone Spread -o ./build/spread.js",
    "build:lib": "browserify ./src/index.js -t [ babelify --presets [ es2015 ] ] -t [ stringify --extensions [.html .css] ] -o ./index.js",
    "watch": "watchify ./src/index.js -t [ babelify --presets [ es2015 ] ] -t [ stringify --extensions [.html .css] ]  --standalone Spread -o ./build/spread.js",
    "watch:lib": "watchify ./src/index.js -t [ babelify --presets [ es2015 ] ] -t [ stringify --extensions [.html .css] ] -o ./index.js",
    "watch:all": "npm run watch & npm run watch:lib"
  },
  "main": "./index.js",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "watchify": "^3.7.0"
  }
}
