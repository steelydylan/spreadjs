{
  "name": "spread-js",
  "version": "0.0.2",
  "main": "./index.js",
  "scripts": {
    "build": "browserify ./src/index.js -t [ babelify --presets [ es2015 ] ] -t [ stringify --extensions [.html .css] ] --standalone Spread -o ./build/spread.js",
    "build:lib": "babel ./src/index.js --presets es2015 --out-file ./index.js",
    "watch": "watchify ./src/index.js -t [ babelify --presets [ es2015 ] ] -t [ stringify --extensions [.html .css] ]  --standalone Spread -o ./build/spread.js",
    "watch:lib": "watch 'npm run build:lib' ./src/",
    "watch:all": "npm run watch & npm run watch:lib",
    "format": "standard-format -w ./src/index.js"
  },
  "dependencies": {
    "a-template": "0.0.9",
    "clone": "^2.0.0",
    "stringify": "^5.1.0",
    "zepto-browserify": "^1.1.6-1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "watchify": "^3.7.0",
    "watch": "^1.0.1"
  }
}
